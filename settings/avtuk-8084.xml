      <!--Описываем протокол Modbus-->
      <modbus>
        
        <!--Необходимо стараться описывать всё группами-->
        <!--Группа регистров, id обязателен-->
        <group id="baseSerialInfo">
          <!--Используемая функция-->
          <function>04</function>
          <!--Конечный тип данных-->
          <type>uint32</type>
          <!--Стартовый адрес-->
          <start-addr>1</start-addr>
          <!--Количество регистров-->
          <count>30</count>
        </group>
        
        <group id="tempHardware">
            <function>04</function>
            <type>float</type>
            <start-addr>101</start-addr>
            <count>4</count>
        </group>
        <group id="voltage">
          <function>04</function>
          <type>float</type>
          <start-addr>1000</start-addr>
          <count>32</count>
        </group>
        <group id="amperage">
          <function>04</function>
          <type>float</type>
          <start-addr>1100</start-addr>
          <count>32</count>
        </group>
        <group id="angle">
          <function>04</function>
          <type>float</type>
          <start-addr>2400</start-addr>
          <count>14</count>
        </group>
        <group id="capacity">
          <function>04</function>
          <type>float</type>
          <start-addr>2420</start-addr>
          <count>28</count>
        </group>
        <group id="tempExtern">
          <function>04</function>
